    "@react-native-firebase/firestore": "^18.8.0",
    "@react-native-firebase/functions": "^18.8.0",
    "@react-native-firebase/storage": "^18.8.0",
    "@react-native-firebase/messaging": "^18.8.0",
    "@react-native-firebase/analytics": "^18.8.0",
    "@react-native-firebase/remote-config": "^18.8.0",
    "@react-native-firebase/crashlytics": "^18.8.0",
    
    "@react-native-google-signin/google-signin": "^10.1.1",
    "@react-native-apple-authentication/apple-authentication": "^2.2.2",
    "react-native-fbsdk-next": "^12.1.2",
    
    "react-native-biometrics": "^3.0.1",
