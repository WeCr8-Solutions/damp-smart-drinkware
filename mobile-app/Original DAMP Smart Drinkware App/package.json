{
  "name": "damp-smart-drinkware",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start",
    "build:web": "expo export --platform web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov --coverageReporters=html",
    "test:coverage:ci": "jest --coverage --coverageReporters=text-lcov --coverageReporters=clover --watchAll=false",
    
    "test:unit": "jest --testPathPattern=tests/unit --verbose",
    "test:unit:watch": "jest --testPathPattern=tests/unit --watch",
    "test:unit:coverage": "jest --testPathPattern=tests/unit --coverage",
    
    "test:integration": "jest --testPathPattern=tests/integration --runInBand --verbose",
    "test:integration:watch": "jest --testPathPattern=tests/integration --runInBand --watch",
    "test:integration:firebase": "jest --testPathPattern=tests/integration/firebase --runInBand",
    "test:integration:ble": "jest --testPathPattern=tests/integration/ble --runInBand",
    
    "test:performance": "jest --testPathPattern=tests/performance --runInBand --maxWorkers=1",
    "test:performance:profile": "jest --testPathPattern=tests/performance --runInBand --logHeapUsage",
    "test:performance:reassure": "reassure --baseline && reassure --compare",
    
    "test:accessibility": "jest --testPathPattern=tests/accessibility --verbose",
    "test:accessibility:audit": "jest --testPathPattern=tests/accessibility --verbose --reporters=default --reporters=jest-html-reporters",
    
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:build": "detox build --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:android:build": "detox build --configuration android.emu.debug",
    "test:e2e:ci": "detox test --configuration ios.sim.release --cleanup",
    
    "test:firebase": "firebase emulators:exec --only firestore,auth,functions 'npm run test:integration:firebase'",
    "test:firebase:setup": "firebase setup:emulators:firestore && firebase setup:emulators:auth",
    
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:accessibility",
    "test:all:coverage": "npm run test:coverage && npm run test:integration && npm run test:performance",
    "test:ci": "npm run lint && npm run test:coverage:ci && npm run test:e2e:ci",
    "test:ci:fast": "npm run lint && npm run test:unit:coverage",
    
    "test:debug": "jest --testPathPattern=tests/unit --runInBand --detectOpenHandles --forceExit",
    "test:clear-cache": "jest --clearCache",
    "test:update-snapshots": "jest --updateSnapshot",
    
    "test:stress": "jest --testPathPattern=tests/performance --runInBand --maxWorkers=1 --testTimeout=60000",
    "test:smoke": "jest --testPathPattern='.*\\.(smoke|sanity)\\.test\\.(ts|tsx)$'",
    
    "performance:baseline": "reassure --baseline",
    "performance:measure": "reassure --compare",
    "performance:report": "reassure --report"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.49.1",
    "firebase": "^11.1.0",
    "@react-native-firebase/app": "^21.7.1",
    "@react-native-firebase/auth": "^21.7.1",
    "@react-native-firebase/firestore": "^21.7.1",
    "@react-native-firebase/functions": "^21.7.1",
    "@react-native-firebase/messaging": "^21.7.1",
    "@react-native-firebase/storage": "^21.7.1",
    "@react-native-firebase/analytics": "^21.7.1",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "expo-location": "~18.1.1",
    "expo-task-manager": "~12.2.1",
    "expo-notifications": "~0.30.6",
    "ajv": "^8.17.1",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.3",
    "expo-camera": "~16.1.5",
    "expo-constants": "~17.1.3",
    "expo-device": "^7.1.4",
    "expo-font": "~13.2.2",
    "expo-haptics": "~14.1.3",
    "expo-image-picker": "~16.1.0",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "~7.1.3",
    "expo-router": "~5.0.2",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.1.5",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.1",
    "react-native-ble-plx": "^3.5.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.30.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.0",
    
    "@testing-library/react-native": "^12.8.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.2",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "@faker-js/faker": "^9.0.3",
    
    "detox": "^20.28.3",
    "@detox/cli": "^20.28.3",
    "detox-expo-helpers": "^0.6.0",
    
    "@firebase/testing": "^0.20.11",
    "firebase-admin": "^12.7.0",
    "firebase-tools": "^13.28.0",
    
    "react-native-ble-plx-mock": "^3.2.0",
    "expo-location-mock": "^1.0.0",
    
    "reassure": "^1.0.0",
    "flipper-plugin-react-native-performance": "^1.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    
    "@axe-core/react-native": "^4.10.2",
    "react-native-accessibility-info": "^3.5.0",
    "axe-core": "^4.10.2",
    
    "@storybook/react-native": "^8.3.6",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-react-native-web": "^8.3.6",
    "@storybook/addon-a11y": "^8.3.6",
    
    "msw": "^2.6.4",
    "cross-env": "^7.0.3",
    "concurrently": "^9.1.0",
    "wait-on": "^8.0.1",
    "tree-kill": "^1.2.2",
    
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0"
  }
}
